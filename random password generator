import random


lower='qwertyuioplkjhgfdsazxcvbnm'
upper='ASDFGHJKLPOIUYTREWQZXCVBNM'
number='123456789'
symbols='!@#$%^&*'

string=lower+upper+number+symbols
getnoofdigit=int(input("enter the number of digits required in password:"))
password=''.join(random.sample(string,k=getnoofdigit))
print(password)
